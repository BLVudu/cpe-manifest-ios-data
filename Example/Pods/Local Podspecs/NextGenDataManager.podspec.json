{
  "name": "NextGenDataManager",
  "version": "0.0.1",
  "summary": "Manifest.XML parser and full one-to-one mapping of the Manifest and Common Metadata specs to Swift objects",
  "license": "Apache License, Version 2.0",
  "homepage": "https://bitbucket.org/wbdigital/nextgen-data",
  "authors": {
    "Alec Ananian": "alec.ananian@warnerbros.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "libraries": "xml2",
  "source": {
    "git": "https://bitbucket.org/wbdigital/nextgen-data.git",
    "commit": "fc34138e842d15d6876576095db26c813a5f96f4"
  },
  "source_files": [
    "ios/Source/Base/*.swift",
    "ios/Source/*.swift"
  ],
  "preserve_paths": "ios/Source/libxml/module.modulemap",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2",
    "SWIFT_INCLUDE_PATHS": "$(SRCROOT)/NextGenDataManager/ios/Source/libxml"
  }
}
